    "import pandas as pd\n",
    "import numpy as np\n",
    "price = pd.read_csv('price.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 124,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "price['return'] = np.log(price['Close']) - np.log(price['Close'].shift(1))\n",
    "\n",
    "price.ix [price.Symbol!= price.Symbol.shift(1),'return'] = np.nan  "
   ]
